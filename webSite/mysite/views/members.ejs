<html>
    <body>
    
        <h1>Subscriptions</h1>
        <a href="http://localhost:3000/subscriptions/"><button>All Members </button></a>
        <a href="http://localhost:3000/subscriptions/edit/?obj=0"><button>Add Members </button></a>
        </br> 
        <h4>Add new Member </h4>
        <% if(data === 0){ %>
        <form action ="/subscriptions/create" method="POST">
            Name: <input name="Name" type="text" >   </br>
            Email: <input name="Email" type="text">   </br>
            City: <input name="City" type="text"  >   </br>
            <button type="submit" name=save value="save">Save</button>
            <button type="submit" name=cancel value="cancel">Cancel</button>
            <% }else{  %>
            <form action ="/subscriptions/create" method="POST">
            Name1: <input name="Name" type="text"  value="<%= data.Name %>" >   </br>
            Email: <input name="Email" type="text" value="<%= data.Email %>">   </br>
            City: <input name="City" type="text"   value="<%= data.City %>">   </br>
            <input name="id" type="hidden"   value="<%= data._id %>" > 
            <button type="submit" name=save value="save">Update</button>
            <button type="submit" name=cancel value="cancel">Cancel</button>
            <% } %>

        </form>  

    </body>
</html>